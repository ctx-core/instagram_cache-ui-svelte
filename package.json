{
	"name": "@ctx-core/instagram_cache-ui-svelte",
	"version": "1.3.339",
	"description": "instagram scraper, account cache, & gallery component",
	"keywords": [
		"ctx-core",
		"instagram",
		"cache",
		"ui",
		"sveltejs"
	],
	"homepage": "https://github.com/ctx-core/instagram_cache-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-instagram_cache-ui-svelte@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./InstagramGallery"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/fontawesome-ui-svelte": "1.1.229",
		"@ctx-core/instagram_cache": "^15.1.308",
		"ctx-core": "^7.2.14",
		"svelte": "^4.2.17"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.8.0",
		"tsx": "^4.11.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"sideEffects": false
}