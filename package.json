{
	"name": "@ctx-core/instagram_cache-ui-svelte",
	"version": "1.0.13",
	"description": "instagram scraper, account cache, & gallery component",
	"homepage": "https://github.com/ctx-core/instagram_cache-ui-svelte#readme",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"cache-instagram": "./bin/cache-instagram.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build",
		"start": "./bin/instagram_cache.js"
	},
	"dependencies": {
		"@ctx-core/fontawesome-ui-svelte": "workspace:^1.0.14",
		"@ctx-core/instagram_cache": "workspace:^10.0.11",
		"@ctx-core/object": "workspace:^22.0.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/selenium-webdriver": "^4.0.16",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.4.12",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"svelte": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/instagram_cache-ui-svelte.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/instagram_cache-ui-svelte/issues"
	}
}
